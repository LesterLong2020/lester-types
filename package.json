{
  "name": "letser-app",
  "version": "1.0.0",
  "description": "app of lester",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.js --progress",
    "build": "cross-env NODE_ENV=production webpack --config webpack/build.js --progress",
    "lint": "eslint --fix --ext .js,.jsx,.ts,.tsx src/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js.jsx,ts,tsx,json,html,less,css,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js.jsx,ts,tsx}": "npm run lint"
  },
  "author": "lester",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "antd-mobile": "^2.3.4",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "fastclick": "^1.0.6",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/fastclick": "^1.0.29",
    "@types/react-transition-group": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "cache-loader": "^4.1.0",
